{
  "version": 2,
  "name": "finance-live-platform",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://${VERCEL_URL}/api",
    "NEXT_PUBLIC_WS_URL": "wss://${VERCEL_URL}"
  }
}